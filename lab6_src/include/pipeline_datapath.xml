<?xml version="1.0" encoding="UTF-8"?>
<nf:module xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
	<nf:name>pipeline_datapath</nf:name>
	<nf:prefix>pipeline</nf:prefix>
	<nf:location>udp</nf:location>
	<nf:description>Registers for Pipeline</nf:description>
	<nf:blocksize>64</nf:blocksize>
  <nf:registers>

    <!-- SOFTWARE REGISTERS -->
    <nf:register>
        <nf:name>interface_pipeline_enable</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_ins_we</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_ins_waddr</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_ins_wdata</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

     <nf:register>
        <nf:name>interface_data_re</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_data_we</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_data_wdata_1</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_data_wdata_0</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>

    <nf:register>
        <nf:name>interface_data_addr</nf:name>
        <nf:type>generic_software32</nf:type>
    </nf:register>


    <!-- HARDWARE REGISTERS -->
    <nf:register>
        <nf:name>interface_data_rdata_1</nf:name>
        <nf:type>generic_hardware32</nf:type>
    </nf:register>

     <nf:register>
        <nf:name>interface_data_rdata_0</nf:name>
        <nf:type>generic_hardware32</nf:type>
    </nf:register>

  </nf:registers>
</nf:module>